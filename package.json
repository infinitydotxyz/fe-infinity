{
  "name": "fe-infinity",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run update:lib && next dev",
    "build": "npm run update:lib && rimraf ./build && cross-env next build",
    "start": "next start",
    "lint": "eslint . --fix",
    "format": "prettier --write {src,pages}/**/{**,}/*.{tsx,ts}",
    "update:lib": "npm install @infinityxyz/lib-frontend@latest",
    "update": "npx npm-check-updates --interactive --format group",
    "analyze": "ANALYZE=true next build",
    "find:unused": "next-unused",
    "deploy:dev": "npm run build && gcloud app deploy -q --appyaml=./app.yaml --project=nftc-dev",
    "deploy:prod": "npm run build && gcloud app deploy -q --appyaml=./app.yaml --project=nftc-infinity"
  },
  "engines": {
    "node": "16.15.0"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.8",
    "@infinityxyz/lib-frontend": "^1.315.1",
    "@next/bundle-analyzer": "^13.1.6",
    "@visx/legend": "2.18.0",
    "@visx/react-spring": "^3.0.0",
    "@visx/tooltip": "2.17.0",
    "@visx/xychart": "2.18.0",
    "axios": "1.2.5",
    "connectkit": "^1.1.2",
    "cross-env": "^7.0.3",
    "d3": "^7.8.2",
    "date-fns": "^2.29.3",
    "discord-markdown": "^2.5.1",
    "eslint-config-next": "^13.1.6",
    "ethers": "5.7.2",
    "html-react-parser": "^3.0.8",
    "logrocket": "^3.0.1",
    "lru-cache": "^7.14.1",
    "merkletreejs": "^0.3.9",
    "next": "^13.1.6",
    "next-themes": "^0.2.1",
    "nprogress": "^0.2.0",
    "postcss-import": "^15.1.0",
    "query-string": "^8.1.0",
    "rc-tooltip": "^5.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-intersection-observer": "^9.4.1",
    "react-resize-detector": "^8.0.3",
    "react-spinners": "^0.13.8",
    "react-spring": "^9.6.1",
    "react-toastify": "^9.1.1",
    "swr": "^1.3.0",
    "tailwind-merge": "^1.8.1",
    "timeago.js": "^4.0.2",
    "wagmi": "^0.10.15"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@next/eslint-plugin-next": "^13.1.6",
    "@types/d3": "^7.4.0",
    "@types/node": "^18.11.18",
    "@types/nprogress": "^0.2.0",
    "@types/react": "18.0.27",
    "@types/react-datepicker": "^4.8.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jsx": "^0.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.1",
    "next-unused": "^0.0.6",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "sass": "^1.57.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4"
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "next-unused": {
    "include": [
      "src"
    ]
  }
}
