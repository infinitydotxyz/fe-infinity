{
  "name": "fe-infinity",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run update:lib && next dev",
    "build": "npm install && npm run update:lib && rimraf ./build && cross-env next build",
    "start": "next start",
    "lint": "eslint . --fix",
    "format": "prettier --write {src,pages}/**/{**,}/*.{tsx,ts}",
    "update:lib": "npm install @infinityxyz/lib-frontend@latest",
    "analyze": "ANALYZE=true next build",
    "find:unused": "next-unused",
    "deploy:dev": "npm run build && gcloud app deploy -q --appyaml=./app.yaml --project=nftc-dev",
    "deploy:prod": "npm run build && gcloud app deploy -q --appyaml=./app.yaml --project=nftc-infinity"
  },
  "engines": {
    "node": "16.15.0"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.5",
    "@heroicons/react": "^1.0.6",
    "@infinityxyz/lib-frontend": "^1.291.0",
    "@next/bundle-analyzer": "^13.0.6",
    "@visx/tooltip": "2.16.0",
    "@visx/xychart": "2.16.0",
    "axios": "0.27.2",
    "connectkit": "^1.1.2",
    "cross-env": "^7.0.3",
    "d3": "^7.8.0",
    "date-fns": "^2.29.3",
    "ethers": "5.7.2",
    "logrocket": "^3.0.1",
    "lru-cache": "^7.14.1",
    "merkletreejs": "^0.3.9",
    "next": "12.3.1",
    "next-themes": "^0.2.1",
    "postcss-import": "^15.1.0",
    "query-string": "^7.1.3",
    "rc-tooltip": "^5.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-intersection-observer": "^9.4.1",
    "react-resize-detector": "^7.1.2",
    "react-spinners": "^0.13.7",
    "react-toastify": "^9.1.1",
    "swr": "^1.3.0",
    "tailwind-merge": "^1.8.0",
    "timeago.js": "^4.0.2",
    "wagmi": "^0.10.15"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@next/eslint-plugin-next": "^13.1.1",
    "@types/d3": "^7.4.0",
    "@types/node": "^17.0.27",
    "@types/react": "17.0.43",
    "@types/react-datepicker": "^4.4.2",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx": "^0.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "next-unused": "^0.0.6",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "sass": "^1.56.2",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4"
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "next-unused": {
    "include": [
      "src"
    ]
  }
}
