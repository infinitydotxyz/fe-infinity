{
  "name": "infinity-fe",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run update:lib && APP_ENV=local next dev",
    "build": "npm install && npm run update:lib && rimraf ./build && cross-env APP_ENV=local next build",
    "start": "APP_ENV=local next start -p 8080",
    "lint": "next lint --fix",
    "format": "prettier --write src/**/*.{tsx,ts}",
    "update:lib": "npm update @infinityxyz/lib"
  },
  "dependencies": {
    "@infinityxyz/lib": "^1.32.4",
    "cross-env": "^7.0.3",
    "ethers": "5.6.1",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-flatpickr": "3.10.9",
    "react-icons": "^4.3.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@headlessui/react": "0.0.0-insiders.79b3015",
    "@heroicons/react": "^1.0.6",
    "@tailwindcss/forms": "^0.5.0",
    "@types/node": "17.0.23",
    "@types/react": "17.0.43",
    "@types/react-flatpickr": "3.8.6",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "autoprefixer": "^10.4.4",
    "eslint": "^8.11.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "8.5.0",
    "postcss": "^8.4.12",
    "prettier": "2.6.1",
    "sass": "1.49.8",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.3"
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "none"
  }
}
