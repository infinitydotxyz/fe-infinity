{
  "name": "fe-infinity",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run update:lib && next dev",
    "build": "npm install && npm run update:lib && rimraf ./build && cross-env next build",
    "start": "next start",
    "lint": "eslint . --fix",
    "format": "prettier --write {src,pages}/**/{**,}/*.{tsx,ts}",
    "update:lib": "npm install @infinityxyz/lib-frontend@latest",
    "analyze": "ANALYZE=true next build",
    "deploy:dev": "npm run build && gcloud app deploy -q --appyaml=./app.yaml --project=nftc-dev",
    "deploy:prod": "npm run build && gcloud app deploy -q --appyaml=./app.yaml --project=nftc-infinity"
  },
  "engines": {
    "node": "16.x"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.6",
    "@heroicons/react": "^1.0.6",
    "@infinityxyz/lib-frontend": "^1.142.1",
    "@next/bundle-analyzer": "^12.2.3",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/typography": "^0.5.4",
    "axios": "0.27.2",
    "cross-env": "^7.0.3",
    "ethers": "5.6.9",
    "firebase": "^9.9.1",
    "formik": "^2.2.9",
    "framer-motion": "^6.5.1",
    "lodash": "^4.17.21",
    "logrocket": "^3.0.1",
    "mitt": "^3.0.0",
    "moment": "^2.29.4",
    "next": "12.2.3",
    "nprogress": "^0.2.0",
    "postcss-import": "^14.1.0",
    "query-string": "^7.1.1",
    "react": "17.0.2",
    "react-avatar-editor": "^13.0.0",
    "react-content-loader": "^6.2.0",
    "react-datepicker": "^4.8.0",
    "react-dom": "17.0.2",
    "react-hot-toast": "^2.3.0",
    "react-icons": "^4.4.0",
    "react-intersection-observer": "^9.4.0",
    "react-resize-detector": "^7.1.2",
    "react-share": "^4.4.0",
    "react-spinners": "^0.13.3",
    "react-toastify": "^9.0.7",
    "react-tooltip": "^4.2.21",
    "recharts": "^2.1.12",
    "swr": "^1.3.0",
    "tailwind-merge": "^1.5.1",
    "timeago.js": "^4.0.2",
    "uuid": "^8.3.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.2",
    "@types/lodash": "^4.14.182",
    "@types/node": "17.0.27",
    "@types/nprogress": "^0.2.0",
    "@types/react": "17.0.43",
    "@types/react-avatar-editor": "^12.0.0",
    "@types/react-datepicker": "^4.4.2",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.20.0",
    "eslint-config-next": "^12.2.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "sass": "^1.54.0",
    "tailwindcss": "^3.1.6",
    "typescript": "^4.7.4"
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "none"
  }
}
