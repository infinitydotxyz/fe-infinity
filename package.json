{
  "name": "infinity-fe",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run update:lib && APP_ENV=local next dev",
    "build": "npm install && npm run update:lib && rimraf ./build && cross-env APP_ENV=local next build",
    "start": "APP_ENV=local next start -p 8080",
    "lint": "next lint",
    "format": "prettier --write src/**/*.{tsx,ts}",
    "update:lib": "npm update @infinityxyz/lib"
  },
  "dependencies": {
    "@infinityxyz/lib": "^1.32.3",
    "cross-env": "^7.0.3",
    "ethers": "5.6.0",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/node": "17.0.23",
    "@types/react": "17.0.43",
    "autoprefixer": "^10.4.4",
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "postcss": "^8.4.12",
    "prettier": "2.5.1",
    "eslint-config-prettier": "8.4.0",
    "@headlessui/react": "0.0.0-insiders.79b3015",
    "@heroicons/react": "^1.0.6",
    "tailwindcss": "^3.0.23",
    "@tailwindcss/forms": "^0.5.0",
    "typescript": "4.6.3"
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "none"
  }
}
